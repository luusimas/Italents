programa {

  funcao inicio() {

    // Declaração das variáveis

    real tempC
    real tempF

    // Temperatura em Celsius (C)

  escreva("Digite a temperatura em graus Celsius: ")
  leia(tempC)

  // Temperatura em Fahrenheit (F)

  tempF = (tempC * 9/5) + 32

  // Resultado

  escreva("A temperatura em Fahrenheit é: ", tempF)

  }
}